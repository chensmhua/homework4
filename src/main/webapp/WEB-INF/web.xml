<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CheckStudentServlet</servlet-name>
  	<display-name>CheckStudentServlet</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.CheckStudentServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentAddServletRedis</servlet-name>
  	<display-name>StudentAddServletRedis</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.StudentAddServletRedis</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentShowServlet</servlet-name>
  	<display-name>StudentShowServlet</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.StudentShowServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentDeleteServlet</servlet-name>
  	<display-name>StudentDeleteServlet</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.StudentDeleteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentUpdateServlet</servlet-name>
  	<display-name>StudentUpdateServlet</display-name>
  	<description></description>
  	<servlet-class>com.biz.servlet.StudentUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CheckStudentServlet</servlet-name>
  	<url-pattern>/CheckStudentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentAddServletRedis</servlet-name>
  	<url-pattern>/StudentAddServletRedis</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentShowServlet</servlet-name>
  	<url-pattern>/StudentShowServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentDeleteServlet</servlet-name>
  	<url-pattern>/StudentDeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentUpdateServlet</servlet-name>
  	<url-pattern>/StudentUpdateServlet</url-pattern>
  </servlet-mapping>
</web-app>
